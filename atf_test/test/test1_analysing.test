<?xml version="1.0"?>
<launch>
        <param name="use_sim_time" value="true" />
    <node name="world_tf_publisher" pkg="cob_gazebo_worlds" type="tf_publisher.py"  respawn="false" output="screen" />

    <param name="analysing/test_config_file" value="$(find atf_test)/config/test_config.yaml" />
    <param name="analysing/test_name" value="ts1_t1" />
    <param name="analysing/test_config" value="test_1" />
    <param name="analysing/result_json_output" value="$(find atf_test)/results/" />
    <param name="analysing/result_yaml_output" value="$(find atf_test)/results/" />

    <test test-name="test1_analysing" pkg="atf_test" type="test_analysing.py" time-limit="300.0" />

    <node pkg="rosbag" type="play" name="player" output="screen" args="--delay=5.0 --clock $(find atf_test)/data/ts1_t1.bag">
        <remap from="/testing/tf" to="/tf" />
    </node>

</launch>
