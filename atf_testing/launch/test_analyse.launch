<?xml version="1.0"?>
<launch>
    <param name="use_sim_time" value="True" />

    <!-- analysis node -->
    <node pkg="atf_testing" type="test_framework.py" name="atf" respawn="false" output="screen" />

    <!-- play recordings -->
    <node pkg="rosbag" type="play" name="player" output="screen" args="--delay=5.0 --clock $(find cob_benchmarking)/results/test_1.bag">
        <remap from="/testing/tf" to="/tf" />
    </node>

</launch>
